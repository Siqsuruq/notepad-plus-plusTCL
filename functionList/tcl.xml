\=========================================================================== -->
<NotepadPlus>
	<functionList>
		<!-- ====================================================== [ Tcl ] -->

		<parser
			displayName="Tcl source"
			id         ="tcl_syntax"
			commentExpr="#"
		>
		<function
				mainExpr="^[\t ]*((proc)[\s]+)[^\n]+\{"
				displayMode="$functionName">
			<functionName>
				<nameExpr expr="[\w: ]+ \{.*\}"/>
			</functionName>
		</function>
		</parser>
	</functionList>
</NotepadPlus>
